    "dev:custom": "node server.js",
    "build": "node scripts/fix-model-exports.js && next build",
    "start": "cross-env NODE_ENV=production node server.js",
    "start:windows": "set NODE_ENV=production& node server.js",
    "start:ip": "node scripts/start-with-ip.js",
    "test": "npm run build && npm run start:windows",
    "test:db": "node scripts/test-db-connection.js",
    "check:ip": "node scripts/check-ip-access.js",
    "init:models": "ts-node -P tsconfig.scripts.json scripts/init-models.ts",
    "verify:db": "ts-node -P tsconfig.scripts.json scripts/verify-db-connection.ts",
    "clear-cache": "rimraf .next/cache",
    "dev:utils": "node scripts/dev-utils.js",
    "lint": "next lint",
    "create-admin": "ts-node -P tsconfig.scripts.json scripts/create-admin.ts",
    "test:admin-login": "ts-node -P tsconfig.scripts.json scripts/test-admin-login.ts",
    "recorder": "ts-node -P tsconfig.scripts.json scripts/start-recorder.ts",
    "test-recorder": "ts-node -P tsconfig.scripts.json scripts/test-recorder.ts",
    "test-integration": "ts-node -P tsconfig.scripts.json scripts/test-integration.ts",
    "test:setup": "powershell -ExecutionPolicy Bypass -File .\\scripts\\setup-dependencies.ps1",
    "test:quick": "ts-node -P tsconfig.scripts.json scripts/quick-test.ts",
    "test:pipeline": "ts-node -P tsconfig.scripts.json scripts/test-full-pipeline.ts",
    "test:component": "ts-node -P tsconfig.scripts.json scripts/component-test.ts",
    "test:playwright": "ts-node -P tsconfig.scripts.json scripts/run-playwright-tests.ts",
    "test:file-host": "ts-node -P tsconfig.scripts.json scripts/test-file-host.ts",
    "test:mixdrop": "ts-node -P tsconfig.scripts.json scripts/test-mixdrop.ts",
    "recorder:start": "ts-node -P tsconfig.scripts.json scripts/start-recorder.ts",
    "test:all": "powershell -ExecutionPolicy Bypass -File .\\scripts\\test-pipeline.ps1",
    "clean": "rimraf .next && rimraf out",
    "clear-cookies": "node scripts/clear-cookies.js",
    "server:ip": "node scripts/run-server-with-ip.js",
    "test:cookies": "node scripts/test-cookies.js"
